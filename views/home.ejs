<%- include('partials/header'); -%>
<div class="home">
<h1 style="display: inline;">Home</h1>
<a href="/compose" style="float: right;">Compose</a>
</div>
<p><%= startingContent %></p>
<% postItems.forEach(function(postItem) { %>
